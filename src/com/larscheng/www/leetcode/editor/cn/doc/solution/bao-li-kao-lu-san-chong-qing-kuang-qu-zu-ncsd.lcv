class Solution {
public:
    int minCharacters(string a, string b) {
        vector<int> vec_a(26),vec_b(26);
        for(int i=0;i<a.length();i++)
            vec_a[a[i]-'a']++;
        for(int i=0;i<b.length();i++)
            vec_b[b[i]-'a']++;
        //ç¬¬ä¸€ç§æƒ…å†µï¼Œa<=åŸºå‡†å­—æ¯ï¼Œb>åŸºå‡†å­—æ¯ï¼ŒåŸºå‡†å­—æ¯æ— æ³•å–z
        int res=INT_MAX,first=INT_MAX,second=INT_MAX,third=INT_MAX;
        for(int i=0;i<25;i++)
        {
            int temp=0;
            //ä¸ç¬¦åˆç¬¬ä¸€ç§æƒ…å†µçš„æƒ…å½¢ï¼ša>åŸºå‡†å­—æ¯,b<=åŸºå‡†å­—æ¯
            for(int j=i+1;j<26;j++)
                temp+=vec_a[j];
            for(int j=0;j<=i;j++)
                temp+=vec_b[j];
            first=min(first,temp);
        }
        //ç¬¬äºŒç§æƒ…å†µï¼Œa>åŸºå‡†å­—æ¯ï¼Œb<=åŸºå‡†å­—æ¯ï¼ŒåŸºå‡†å­—æ¯æ— æ³•å–z
        for(int i=0;i<25;i++)
        {
            int temp=0;
            //ä¸ç¬¦åˆç¬¬äºŒç§æƒ…å†µçš„æƒ…å½¢ï¼ša<=åŸºå‡†å­—æ¯ï¼Œb>åŸºå‡†å­—æ¯
            for(int j=0;j<=i;j++)
                temp+=vec_a[j];
            for(int j=i+1;j<26;j++)
                temp+=vec_b[j];
            second=min(second,temp);
        }
        //ç¬¬ä¸‰ç§æƒ…å†µï¼Œaå’Œbç­‰äºåŸºå‡†å­—æ¯
        for(int i=0;i<26;i++)
        {
            int temp=0;
            for(int j=0;j<26;j++)
            {   
                //j==iæ—¶ï¼Œå³aå’Œbä¸­çš„å­—æ¯ä¸ºåŸºå‡†å­—æ¯ï¼Œä¸éœ€è¦æ”¹å˜
                if(j==i)
                    continue;
                //j!=iæ—¶ï¼Œaå’Œbä¸­çš„å­—æ¯éƒ½è¦æ”¹ä¸ºiå¯¹åº”çš„åŸºå‡†å­—æ¯
                temp+=vec_a[j]+vec_b[j];
            }
            third=min(third,temp);
        }
        res=min(res,first);
        res=min(res,second);
        res=min(res,third);
        return res;
    }
};
