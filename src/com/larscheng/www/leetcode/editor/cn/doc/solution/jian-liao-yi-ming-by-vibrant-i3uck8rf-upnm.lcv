class Solution {
public:
    int numIdenticalPairs(vector<int>& nums) {
        int all=0;
        for(auto p=nums.begin();p<nums.end();p++)
        {
            for(auto pe=p+1;pe<nums.end();pe++)
            {
                if(*p==*pe)
                    all++;
            }

        }
    return all;
    }
};
```

